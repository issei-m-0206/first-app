.wrapper  
  .header
    .header__list
      .header__list__user-name
        = current_user.name
      .header__list__btn  
        = link_to "logout", destroy_user_session_path, method: :delete, class: 'logout'
        =link_to edit_user_path(current_user) do
          edit
  .contents
    .contents__box
      =link_to "posts/new", class: "contents__box__btn-1" do
        =icon('fas', 'map-marked-alt')
        TripLog
      =link_to "/", class: "contents__box__btn-2" do  
        =icon('fas', 'map-pin')     
        NextTrip
      =link_to "/", class: "contents__box__btn-3" do
        =icon('fas', 'users')
        Tourist
        